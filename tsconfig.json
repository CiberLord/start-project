{
    "compilerOptions": {
      "target": "es6",
      "declaration": false,
      "sourceMap": false,
      "strictNullChecks":true,
      "removeComments": true,
      "allowJs": true,
      "strict": false,
      "forceConsistentCasingInFileNames": true,
      "noImplicitThis": true,
      "noImplicitAny": true,
      "baseUrl": "./",
      "moduleResolution": "node",
      "paths": {
        "*": [
          "node_modules/*"
        ]
      },
      "typeRoots": [
        "node_modules/@types"
      ],
      "esModuleInterop": true,
      "experimentalDecorators": true,
      "emitDecoratorMetadata": true,
      "resolveJsonModule": true,
      "jsx": "react"
    },
}
